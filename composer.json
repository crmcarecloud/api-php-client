{
    "name": "crmcarecloud/api-php-client",
    "description": "",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "http://swagger.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "squizlabs/php_codesniffer": "~2.6",
        "phpstan/phpstan": "^1.8"
    },
     "scripts": {
       "phpstan": "vendor/bin/phpstan --memory-limit=-1"
     },
    "autoload": {
        "psr-4": { "CrmCareCloud\\Webservice\\RestApi\\Client\\" : [ "lib/", "src/" ] }
    },
    "autoload-dev": {
        "psr-4": { "CrmCareCloud\\Webservice\\RestApi\\Client\\" : "test/" }
    }
}
