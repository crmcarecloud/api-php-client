{
    "name": "crmcarecloud/api-php-client",
    "description": "",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://swagger.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.19.3",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "phpstan/phpstan": "^1.8"
    },
     "scripts": {
       "phpstan": "vendor/bin/phpstan"
     },
    "autoload": {
        "psr-4": { "CrmCareCloud\\Webservice\\RestApi\\Client\\" : [ "lib/", "src/" ] }
    },
    "autoload-dev": {
        "psr-4": { "CrmCareCloud\\Webservice\\RestApi\\Client\\" : "test/" }
    }
}
